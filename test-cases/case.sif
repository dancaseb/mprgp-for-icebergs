Check Keywords "Warn"
Header
  Mesh DB "." "square1"
End

Simulation
  Max Output Level = 10
  Coordinate System = "Cartesian"

  Simulation Type = Steady
  Output Intervals = 1
  Steady State Max Iterations = 1

  Post File = case_default_solver.vtu
  Ascii Output = True

  Mesh Levels = 1 ! This sets the mesh refinement level. level 2 splits each line into two lines -> doubles the complexity
End

Body 1
  Equation = 1
  Body Force = 1
  Material = 1
End

Equation 1
  Name = "Heat"
  Active Solvers(1) = 1
End

Body Force 1
!Heat Source = Variable Coordinate 1
!    Real MATC "100*sin(2*pi*tx)"

!  Temp Upper Limit = Variable "Coordinate 1"
!    Real MATC "sin(4*pi*tx)"
!  Temp Upper Limit = Real 1.5
  Temp Lower Limit = Real -2.0

Heat Source = Variable Coordinate 1
  Real MATC "100*sin(2*pi*tx)"

!  Temp Lower Limit = Real -0.5
!  Temp Upper Limit = Real 1.5
  

End


Material 1
  Name = "Ideal"
  diffusion coefficient = real 1.0
End 


Solver 1
  Equation = ModelPDE
  Variable = Temp
  Procedure = "MyPDE2_default" "DefaultSolver"
  My linear solver = Logical False


  Apply Limiter = Logical True
  Limiter Load Tolerance = Real 1.0e-6
  Limiter Value Tolerance = Real 1.0e-8

  Steady State Convergence Tolerance = 1.0e-5

  Nonlinear System Convergence Tolerance = 1.0e-5
  Nonlinear System Max Iterations = 30
  Nonlinear System Newton After Iterations = 3
  Nonlinear System Newton After Tolerance = 1.0e-5
  Nonlinear System Relaxation Factor = 1
  Nonlinear System Convergence Measure = solution   

  Linear System Solver = Iterative
  Linear System Iterative Method = BiCGStab
  Linear System Max Iterations = 500
  Linear System Convergence Tolerance = 1.0e-8
  Linear System Preconditioning = none
  Linear System ILUT Tolerance = 1.0e-3
  Linear System Abort Not Converged = False
  Linear System Residual Output = 10
  Linear System Precondition Recompute = 1
  Linear System Symmetric = True
  Linear System Save = True


End 


!Boundary Condition 1
!  Name = Sides
!  Target Boundaries(2) = 2 4
!  Temp = Real 0.0
!!  Temp Upper Limit = Real 0.0
!End
!
!Boundary Condition 2
!  Name = Lower
!  Target Boundaries = 1
!  Temp Lower Limit = Real -0.5
!!  Temp = Real -0.5
!End
!
!Boundary Condition 3
!  Name = Upper
!  Target Boundaries = 3
!  Temp Lower Limit = Real -2.0
!!  Temp = Real 1.5
!End

Boundary Condition 1
  Target Boundaries = 1
  Temp = Real 0.0
End

RUN
